{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "pdf-stamper",
  "version": "0.1.7",
  "identifier": "com.pdf-stamper.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "pdf-stamper",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    },
    "dialog": null
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon-32.png",
      "icons/icon-128.png"
    ]
  }
}
